demos:
	@echo a list of 10 men and 10 women
	python demo_bottom_up.py -e 4 -r 20 male:10 female:10
	python demo_bottom_up.py -e 1 -r 20 male:10 female:10
	python demo_bottom_up.py -e .1 -r 20 male:10 female:10
	@echo a list of just 10 men
	python demo_bottom_up.py -e 4 -r 20 male:10 female:0
	python demo_bottom_up.py -e 1 -r 20 male:10 female:0
	python demo_bottom_up.py -e .1 -r 20 male:10 female:0
